SPRING_PROFILES_ACTIVE=local

# ── Database (docker-compose에서 override됨, local 개발시 사용) ──
DB_URL=jdbc:mysql://localhost:3306/doktoridb?serverTimezone=Asia/Seoul&useSSL=false
DB_USERNAME=root
DB_PASSWORD=your_password_here

# ── JWT ──
JWT_SECRET=your_jwt_secret_here

# ── Kakao OAuth ──
KAKAO_CLIENT_ID=your_kakao_client_id
KAKAO_CLIENT_SECRET=your_kakao_client_secret
KAKAO_REDIRECT_URI=http://localhost:8080/api/oauth/kakao/callback
KAKAO_FRONTEND_REDIRECT=http://localhost:3000/oauth/callback
KAKAO_REST_API_KEY=your_kakao_rest_api_key

# ── Zoom ──
ZOOM_ACCOUNT_ID=your_zoom_account_id
ZOOM_CLIENT_ID=your_zoom_client_id
ZOOM_CLIENT_SECRET=your_zoom_client_secret

# ── AI Service ──
AI_API_KEY=your_ai_api_key

# ── Swagger Docs Auth ──
DOCS_AUTH_USERNAME=doktori
DOCS_AUTH_PASSWORD=your_docs_password
